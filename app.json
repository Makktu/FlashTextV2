{
  "expo": {
    "name": "FlashText",
    "slug": "flashtext",
    "version": "2.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "bundleIdentifier": "com.flashtext.mcnamara",
      "supportsTablet": true,
      "buildNumber": "12",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app does not require access to your microphone."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "a0242b8a-7123-4a97-abff-1d8d22ffea18"
      }
    }
  }
}
